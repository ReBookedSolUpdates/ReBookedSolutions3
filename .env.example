# ReBooked Solutions Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration (Required)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Paystack Payment Configuration (Required for production)
VITE_PAYSTACK_PUBLIC_KEY=pk_test_your_paystack_public_key
VITE_PAYSTACK_SECRET_KEY=sk_test_your_paystack_secret_key

# Application URL (Required for production)
VITE_APP_URL=https://rebookedsolutions.co.za

# Delivery Service API Keys (Optional - for shipping features)
VITE_COURIER_GUY_API_KEY=your_courier_guy_api_key
VITE_FASTWAY_API_KEY=your_fastway_api_key
VITE_SHIPLOGIC_API_KEY=your_shiplogic_api_key

# Development Settings (Optional)
NODE_ENV=development
VITE_DEBUG=false
VITE_ENABLE_ANALYTICS=true

# Important Notes:
# 1. Never commit actual API keys to version control
# 2. For production deployment, set these variables in your hosting platform
# 3. Make sure all URLs use HTTPS in production
# 4. Keep your secret keys secure and rotate them regularly
